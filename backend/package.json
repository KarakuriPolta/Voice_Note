{
    "name": "call-assistant-backend",
    "version": "1.0.0",
    "description": "Backend for Call Assistant",
    "main": "index.js",
    "scripts": {
        "start": "electron ."
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@electron/remote": "^2.1.2",
        "@google-cloud/speech": "^7.0.0",
        "@google-cloud/vertexai": "^1.10.0",
        "axios": "^1.6.7",
        "body-parser": "^2.2.0",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "ws": "^8.18.1"
    },
    "devDependencies": {
        "electron": "^36.2.1",
        "electron-builder": "^26.0.12"
    },
    "build": {
        "appId": "com.poltergeist-lab.call-assistant",
        "productName": "Call Assistant",
        "directories": {
            "output": "dist"
        },
        "files": [
            "**/*",
            "!dist/**/*",
            "!node_modules/**/*",
            "node_modules/**/*"
        ],
        "extraResources": [
            {
                "from": "google-credentials.json",
                "to": "google-credentials.json"
            },
            {
                "from": "../frontend",
                "to": "frontend"
            }
        ],
        "win": {
            "target": "nsis"
        }
    }
}
